{
  "name": "wee",
  "version": "5.0.1",
  "description": "Blueprint for modern web development",
  "homepage": "https://www.weepower.com",
  "license": "Apache-2.0",
  "moduleRoots": "node_modules/wee-core/scripts",
  "keywords": [
    "css",
    "framework",
    "javascript"
  ],
  "repository": "weepower/wee",
  "bugs": "https://github.com/weepower/wee/issues",
  "browserslist": [
    "last 2 versions",
    "safari >= 10",
    "ie 11"
  ],
  "config": {
    "root": "public",
    "assets": "assets",
    "source": "source",
    "components": "source/components",
    "build": "build"
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_root/$npm_package_config_assets",
    "lint": "eslint --ext .js,.vue source",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run watch",
    "prebuild": "node $npm_package_config_build/prebuild.js",
    "build": "cross-env NODE_ENV=development BASE_PATH=$npm_package_config_source webpack --config $npm_package_config_build/webpack.dev.config.js --progress --hide-modules",
    "build:prod": "npm run clean && cross-env NODE_ENV=production BASE_PATH=$npm_package_config_source webpack -p --config $npm_package_config_build/webpack.prod.config.js --progress --hide-modules",
    "watch": "npm run clean && npm run build -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "autoprefixer": "^9.4.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-vue": "^5.1.0",
    "hash-sum": "^1.0.2",
    "imagemin-webpack-plugin": "^2.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.5.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "suppress-chunks-webpack-plugin": "0.0.5",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-loader": "^15.6.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-chain": "^5.2.0",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "wee-cli": "^2.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "es6-promise": "^4.2.5",
    "path-to-regexp": "1.7.0",
    "vue": "^2.5.22",
    "wee-core": "^2.2.22"
  }
}