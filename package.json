{
  "name": "wee",
  "version": "4.2.0",
  "description": "Blueprint for modern web development",
  "homepage": "https://www.weepower.com",
  "license": "Apache-2.0",
  "moduleRoots": "node_modules/wee-core/scripts",
  "keywords": [
    "css",
    "framework",
    "javascript"
  ],
  "repository": "weepower/wee",
  "bugs": "https://github.com/weepower/wee/issues",
  "browserslist": [
    "last 2 versions",
    "ie > 9"
  ],
  "config": {
    "root": "public",
    "source": "source",
    "build": "build",
    "tasks": "build/tasks"
  },
  "scripts": {
    "prebuild": "node build/prebuild.js",
    "build": "cross-env NODE_ENV=development BASE_PATH=$npm_package_config_source webpack --config $npm_package_config_build/webpack.dev.config.js --progress --hide-modules",
    "build:prod": "cross-env NODE_ENV=production BASE_PATH=$npm_package_config_source webpack -p --config $npm_package_config_build/webpack.prod.config.js --progress --hide-modules",
    "watch": "npm run build -- --watch",
    "analyze": "npm run build -- --env.analyze"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "async": "^2.1.4",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.1",
    "imagemin-webpack-plugin": "^1.6.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.11",
    "postcss-assets-webpack-plugin": "^1.1.0",
    "postcss-comment": "^1.0.3",
    "postcss-loader": "^2.0.10",
    "postcss-variable-media": "^1.0.3",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1",
    "webpack-uglify-js-plugin": "^1.1.9",
    "wee-cli": "next"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "path-to-regexp": "^1.7.0",
    "vue": "^2.0.1",
    "wee-core": "2.1.8"
  }
}
